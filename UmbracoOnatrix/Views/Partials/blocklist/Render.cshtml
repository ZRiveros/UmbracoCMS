@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<IEnumerable<Umbraco.Cms.Core.Models.Blocks.BlockListItem>>

@foreach (var block in Model)
{
    if (block?.Content == null) { continue; }

    var alias = block.Content.ContentType.Alias;
    @await Html.PartialAsync($"blocklist/Components/{alias}", block)
}
